{
	"builds": [
	  {
		"src": "package.json",
		"use": "@vercel/static-build",
		"config": {
		  "distDir": "build"
		}
	  }
	],
	"routes": [
	  {
		"src": "/(.*)",
		"dest": "/$1"
	  },
	  {
		"src": "/:path*",
		"has": [
		  {
			"type": "host",
			"value": "bookbans.citizencodex.com"
		  }
		],
		"dest": "/our-stories/book-bans/:path*"
	  }
	]
  }
  