{
	"builds": [
	  {
		"src": "package.json",
		"use": "@vercel/static-build",
		"config": {
		  "distDir": "build"
		}
	  }
	],
	"rewrites": [
	  {
		"source": "/book-bans/",
		"destination": "https://projects.citizencodex.com/book-bans/"
	  },
	  {
		"source": "/book-bans/:match*",
		"destination": "https://projects.citizencodex.com/book-bans/:match*"
	  },
	  {
		"source": "/",
		"destination": "https://projects.citizencodex.com/book-bans/"
	  },
	  {
		"source": "/:match*",
		"destination": "https://projects.citizencodex.com/book-bans/:match*"
	  }
	]
  }
  