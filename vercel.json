{
	"builds": [
	  {
		"src": "package.json",
		"use": "@vercel/static-build",
		"config": {
		  "distDir": "build"
		}
	  }
	],
	"rewrites": [
	  {
		"source": "/our-stories/book-bans/:subdir*",
		"destination": "https://bookbans.citizencodex.com/:subdir*"
	  },
	  {
		"source": "/(.*)",
		"destination": "/"
	  }
	],
	"cleanUrls": true, 
	"trailingSlash": false
  }
  